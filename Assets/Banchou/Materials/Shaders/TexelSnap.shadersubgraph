    "m_ObjectId": "d150667e7dc74ebba201834086a417bf",
    "m_Group": {
        "m_Id": ""
    },
    "m_Name": "TexelSnap (Custom Function)",
    "m_DrawState": {
        "m_Expanded": true,
        "m_Position": {
            "serializedVersion": "2",
            "x": -457.0,
            "y": -13.0,
